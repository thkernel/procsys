<div class="container">
	<div class="row">
	<div class="col-md-12">
	<div class="index-heading pull-left">
	<h3>Liste des traitements de bordereaux</h3>
	</div>
   <div class="pull-right">
   <p>
	<%= link_to raw('<i class="fa fa-plus" aria-hidden="true"></i> Nouveau'), new_delivery_slip_processing_path, class: "btn btn-primary", remote: true %>
	</p>
	</div>
	</div>
 </div>
  <div class="row">
 <div class="col-lg-12">
	<div class="panel panel-default">
		  
			 
					<div class="panel-body">
		<table id="table-list" class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
    <th>Date</th>
      <th>Réf. Bordereau</th>
      <th>Motif</th>
      <th>Délai</th>
      <th>Date traitement</th>
      
      <th >Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @delivery_slip_processings.each do |delivery_slip_processing| %>
      <tr>
      <td><%= delivery_slip_processing.created_at %></td>
        <td><%= delivery_slip_processing.delivery_slip %></td>
        <td><%= delivery_slip_processing.motif %></td>
        <td><%= delivery_slip_processing.time_limit %></td>
        <td><%= delivery_slip_processing.processing_date %></td>
        
        <td><%= link_to 'Show', delivery_slip_processing %></td>
        <td><%= link_to 'Edit', edit_delivery_slip_processing_path(delivery_slip_processing) %></td>
        <td><%= link_to 'Destroy', delivery_slip_processing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

	</div>
	</div>
</div>
</div>
</div>
<div id="motif-modal" class="modal fade"></div>
<div id="user-disable-modal" class="modal fade"></div>
<div id="user-enable-modal" class="modal fade"></div>

<div id="commission-setting-modal" class="modal fade"></div>